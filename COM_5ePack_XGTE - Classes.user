    <tag group="abCategory" tag="ClrChanDiv" name="Cleric Channel Divinity Ability" abbrev="Cleric Channel Divinity Ability"/>
    <tag group="abDuration" tag="Instant" name="Instantaneous" abbrev="inst"/>
